{
  "typescript.tsdk": "./node_modules/typescript/lib",
  "volar.tsPlugin": true,
  "tslint.typeCheck": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "[html]": {"editor.defaultFormatter": null},
  "eslint.validate": [
    "typescript",
    "html",
    "javascript",
    "javascriptreact",
    "vue"
  ],
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "search.exclude": {
    ".git": true,
    ".build": true,
    "**/dist": true,
    "**/Jira.csv": true,
    "**/.DS_Store": true,
    "**/.vscode": true,
    "**/coverage": true,
    "**/node_modules": true
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.validate.enable": true,
  "typescript.check.npmIsInstalled": true,
  "typescript.referencesCodeLens.enabled": false,
  "typescript.implementationsCodeLens.enabled": true,
  "vsintellicode.typescript.completionsEnabled": true,
  "typescript.suggest.autoImports": true,
  "jira-plugin.workingProject": "",
  "atlascode.jira.todoIssues.enabled": true,
  "generalConfiguration.sortOnBeforeSave": true,
  "importStringConfiguration.maximumNumberOfImportExpressionsPerLine.type": "newLineEachExpressionAfterCountLimitExceptIfOnlyOne",
  "importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 80,
  "importStringConfiguration.tabSize": 2,
  "importStringConfiguration.quoteMark": "double",
  "i18n-ally.localesPaths": [
    "packages/xotosphere-application/server/src/assets/locales",
    "packages/xotosphere-design/design/src/assets/locales"
  ],
  "atlascode.jira.todoIssues.triggers": ["todo", "bug", "fixme", "issue"],
  "atlascode.jira.startWorkBranchTemplate.customTemplate": "{{{PREFIX}}}/{{{ISSUEKEY}}}/{{{summary}}}"
}